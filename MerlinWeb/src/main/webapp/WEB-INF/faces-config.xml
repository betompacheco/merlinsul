            <to-view-id>/inclusaomensagem.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <validator>
        <validator-id>yearValidator</validator-id>
        <validator-class>com.merlin.beans.validator.YearValidator</validator-class>
    </validator>
</faces-config>


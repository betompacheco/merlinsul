        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <navigation-rule>
        <from-view-id>/consultaveiculo.jsp</from-view-id>
        <navigation-case>
            <from-outcome>editar</from-outcome>
            <to-view-id>/inclusaoveiculo.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/consultacondominio.jsp</from-view-id>
        <navigation-case>
            <from-outcome>editar</from-outcome>
            <to-view-id>/inclusaocondominio.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/consultaapartamento.jsp</from-view-id>
        <navigation-case>
            <from-outcome>editar</from-outcome>
            <to-view-id>/inclusaoapartamento.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/consultaproprietario.jsp</from-view-id>
